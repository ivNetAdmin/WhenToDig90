<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WhenToDig90.Views.FirstPage">
  
  <ContentPage.ToolbarItems>
    <ToolbarItem Icon="{Binding CalendarIcon}" />
    <ToolbarItem x:Name="Cancel" Text="Cancel" ></ToolbarItem>  
    <ToolbarItem x:Name="Save" Text="Save" ></ToolbarItem>  
  </ContentPage.ToolbarItems>
  
  <ContentPage.Content>
    <Button Text="Navigate to second page" Command="{Binding NavigationCommand}" VerticalOptions="Center" HorizontalOptions="Center" />
  </ContentPage.Content>
</ContentPage>
