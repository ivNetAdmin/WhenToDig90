<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WhenToDig90.Views.JobPage">
  
  <ContentPage.ToolbarItems>
    <ToolbarItem Icon="{Binding CalendarIcon}" Command="{Binding CalendarNavigationCommand}" />
    <ToolbarItem Icon="{Binding JobIcon}" />
    <ToolbarItem Icon="{Binding ReviewIcon}" Command="{Binding ReviewNavigationCommand}" />
    <ToolbarItem Icon="{Binding PlantIcon}" Command="{Binding PlantNavigationCommand}" />
  </ContentPage.ToolbarItems>
  
  <ContentPage.Content>
    <Label Text="Job" VerticalOptions="Center" HorizontalOptions="Center" />
  </ContentPage.Content>
</ContentPage>
